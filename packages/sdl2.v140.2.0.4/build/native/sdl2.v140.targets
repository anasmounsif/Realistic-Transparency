<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" InitialTargets="sdl2_v140_init">
  <PropertyGroup Label="Default initializers for properties">
    <Linkage-sdl2_v140 Condition="'$(Linkage-sdl2_v140)' == ''">dynamic</Linkage-sdl2_v140>
    <CallingConvention-sdl2_v140 Condition="'$(CallingConvention-sdl2_v140)' == ''">cdecl</CallingConvention-sdl2_v140>
  </PropertyGroup>
  <ItemDefinitionGroup Label="Win32 and Release" Condition="'$(Platform.ToLower())' == 'win32' And ( $(Configuration.ToLower().IndexOf('debug')) == -1 )">
    <Link>
      <AdditionalDependencies>$(MSBuildThisFileDirectory)../..//build/native/lib/Win32\Release\SDL2main.lib;$(MSBuildThisFileDirectory)../..//build/native/lib/Win32\Release\SDL2.lib;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
  </ItemDefinitionGroup>
  <ItemDefinitionGroup Label="Win32 and Debug" Condition="'$(Platform.ToLower())' == 'win32' And ( $(Configuration.ToLower().IndexOf('debug')) &gt; -1 )">
    <Link>
      <AdditionalDependencies>$(MSBuildThisFileDirectory)../..//build/native/lib/Win32\Debug\SDL2main.lib;$(MSBuildThisFileDirectory)../..//build/native/lib/Win32\Debug\SDL2.lib;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
  </ItemDefinitionGroup>
  <ItemDefinitionGroup Label="x64 and Release" Condition="'$(Platform.ToLower())' == 'x64' And ( $(Configuration.ToLower().IndexOf('debug')) == -1 )">
    <Link>
      <AdditionalDependencies>$(MSBuildThisFileDirectory)../..//build/native/lib/x64\Release\SDL2main.lib;$(MSBuildThisFileDirectory)../..//build/native/lib/x64\Release\SDL2.lib;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
  </ItemDefinitionGroup>
  <ItemDefinitionGroup Label="x64 and Debug" Condition="'$(Platform.ToLower())' == 'x64' And ( $(Configuration.ToLower().IndexOf('debug')) &gt; -1 )">
    <Link>
      <AdditionalDependencies>$(MSBuildThisFileDirectory)../..//build/native/lib/x64\Debug\SDL2main.lib;$(MSBuildThisFileDirectory)../..//build/native/lib/x64\Debug\SDL2.lib;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
  </ItemDefinitionGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <AdditionalIncludeDirectories>$(MSBuildThisFileDirectory)../..//build/native/include/;%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
    </ClCompile>
    <ResourceCompile>
      <AdditionalIncludeDirectories>$(MSBuildThisFileDirectory)../..//build/native/include/;%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
    </ResourceCompile>
  </ItemDefinitionGroup>
  <ItemGroup>
    <PropertyPageSchema Include="$(MSBuildThisFileDirectory)\default-propertiesui.xml" />
  </ItemGroup>
  <UsingTask TaskName="sdl2_v140_Contains" AssemblyFile="$(MSBuildToolsPath)\Microsoft.Build.Tasks.v4.0.dll" TaskFactory="CodeTaskFactory">
    <ParameterGroup>
      <Text Output="false" ParameterType="System.String" />
      <Library Output="false" Required="true" ParameterType="System.String" />
      <Value Output="false" Required="true" ParameterType="System.String" />
      <Result Output="true" ParameterType="System.String" />
    </ParameterGroup>
    <Task>
      <Code>Result = ((Text ?? "").Split(';').Contains(Library) ) ? Value : String.Empty;</Code>
    </Task>
  </UsingTask>
  <Target Name="sdl2_v140_init">
    <sdl2_v140_Contains Text="Linkage-dynamic" Library="sdl2_v140" Value="dynamic" Condition="'$(Linkage-sdl2_v140)'==''">
      <Output TaskParameter="Result" PropertyName="Linkage-sdl2_v140" />
    </sdl2_v140_Contains>
    <sdl2_v140_Contains Text="Linkage-static" Library="sdl2_v140" Value="static" Condition="'$(Linkage-sdl2_v140)'==''">
      <Output TaskParameter="Result" PropertyName="Linkage-sdl2_v140" />
    </sdl2_v140_Contains>
    <sdl2_v140_Contains Text="Linkage-ltcg" Library="sdl2_v140" Value="ltcg" Condition="'$(Linkage-sdl2_v140)'==''">
      <Output TaskParameter="Result" PropertyName="Linkage-sdl2_v140" />
    </sdl2_v140_Contains>
    <sdl2_v140_Contains Text="Linkage-sxs" Library="sdl2_v140" Value="sxs" Condition="'$(Linkage-sdl2_v140)'==''">
      <Output TaskParameter="Result" PropertyName="Linkage-sdl2_v140" />
    </sdl2_v140_Contains>
    <sdl2_v140_Contains Text="CallingConvention-cdecl" Library="sdl2_v140" Value="cdecl" Condition="'$(CallingConvention-sdl2_v140)'==''">
      <Output TaskParameter="Result" PropertyName="CallingConvention-sdl2_v140" />
    </sdl2_v140_Contains>
    <sdl2_v140_Contains Text="CallingConvention-stdcall" Library="sdl2_v140" Value="stdcall" Condition="'$(CallingConvention-sdl2_v140)'==''">
      <Output TaskParameter="Result" PropertyName="CallingConvention-sdl2_v140" />
    </sdl2_v140_Contains>
    <sdl2_v140_Contains Text="CallingConvention-fastcall" Library="sdl2_v140" Value="fastcall" Condition="'$(CallingConvention-sdl2_v140)'==''">
      <Output TaskParameter="Result" PropertyName="CallingConvention-sdl2_v140" />
    </sdl2_v140_Contains>
    <sdl2_v140_Contains Text="CallingConvention-thiscall" Library="sdl2_v140" Value="thiscall" Condition="'$(CallingConvention-sdl2_v140)'==''">
      <Output TaskParameter="Result" PropertyName="CallingConvention-sdl2_v140" />
    </sdl2_v140_Contains>
    <sdl2_v140_Contains Text="CallingConvention-clrcall" Library="sdl2_v140" Value="clrcall" Condition="'$(CallingConvention-sdl2_v140)'==''">
      <Output TaskParameter="Result" PropertyName="CallingConvention-sdl2_v140" />
    </sdl2_v140_Contains>
  </Target>
</Project>